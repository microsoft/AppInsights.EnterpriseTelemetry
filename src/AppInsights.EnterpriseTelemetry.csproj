<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>AppInsights.EnterpriseTelemetry</PackageId>
    <Title>AppInsights.EnterpriseTelemetry</Title>
    <Version>1.1.0</Version>
    <Authors>Pratik Bhattacharya</Authors>
    <Company>Microsoft</Company>
    <PackageDescription>Library for capturing telemetry in an Enterprise level application</PackageDescription>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>ApplicationInsights;Enterprise Telemetry;Telemetry;Logging;AppInsights;AspNetCore</PackageTags>
    <RepositoryUrl>https://github.com/microsoft/AppInsights.EnterpriseTelemetry</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryBranch>main</RepositoryBranch>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>Preview</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ApplicationInsights.AspNetCore" Version="2.17.0" />
    <PackageReference Include="System.Text.Encodings.Web" Version="5.0.1" />
    <PackageReference Include="System.Text.Json" Version="5.0.1" />
  </ItemGroup>

</Project>
